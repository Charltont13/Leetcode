var increasingTriplet = function(nums) {
    let m = 0;
    for (let i = 0; i < nums.length - 1; i++){
        if(nums[i + 1] > nums[i]){
            m += 1;
        }
        else if(nums[i + 1] === nums[i]){
            m += 0;
        }
        else{
            m = 0;
        }

         
    }
    if(m >= 2 ){
            return true;
        }
        else{
            return false;
        }
};
